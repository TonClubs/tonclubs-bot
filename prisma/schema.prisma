datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Integrations {
  id                Int      @id @default(autoincrement())
  createdAt         DateTime @default(now())
  collectionAddress String
  groupId           Int      @unique
  handle            String   @unique

  @@index([collectionAddress, groupId, handle])
}
